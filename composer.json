{
  "name": "ingenerator/warden-persistence-doctrine",
  "type": "library",
  "description": "Persistence provider for warden using doctrine2",
  "homepage": "http://github.com/ingenerator/warden-persistence-doctrine",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Andrew Coulton",
      "email": "andrew@ingenerator.com",
      "homepage": "http://ingenerator.com"
    }
  ],
  "require": {
    "php": "^5.5||^7.0",
    "ingenerator/warden-core": "^0.1",
    "doctrine/orm": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://php-packages.ingenerator.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ingenerator\\Warden\\Persistence\\Doctrine\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
